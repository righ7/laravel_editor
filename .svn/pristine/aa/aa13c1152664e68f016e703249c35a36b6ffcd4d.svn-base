.content-box{
    width: 100%;
    height: 100%;
    background: white;
    padding: 20px;
}
.content-box-left{
    width: 66%;
}
.titleBox{
    position: relative;
}
.titleInp{
    margin: 10px;
}
.title_tip{
    font-size: 14px;
    position: absolute;
    right: 20px;
    top: 6px;
    color: #999;
}
.title_tip.warning{
    color: #ed4040;
}
.leadsBox{
    position: relative;
}
.leads{
    resize: none;
}
.leads_tip {
    font-size: 14px;
    position: absolute;
    right: 20px;
    bottom: 6px;
    color: #999;
}
.leads_tip.warning{
    color: #ed4040;
}
.threeImgBox{
    display: none;
    overflow: hidden;
}
.addCover,.supCover{
    margin: 10px;
}
.addCover .oneImgBox,.supCover .oneImgBox,.threeImgBox .sinImgBox{
    margin-top: 10px;
    position: relative;
}
.threeImgBox .sinImgBox{
    float: left;
    margin-right: 10px;
}
.addCover .oneImgBox .ImgBox,.supCover .oneImgBox .ImgBox,.threeImgBox .sinImgBox .ImgBox{
    display: inline-block;
    width: 164px;
    /*height: 112px;*/
    /*line-height: 112px;*/
    text-align: center;
    border: 1px solid #e8e8e8;
    background-color: #f0f1f3;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
    cursor: pointer;
    font-size: 40px;
    font-weight: bold;
    color: #ccc;
    position: absolute;
    left: 0;
    top: 0;
}
.addCover .oneImgBox .ImgBox{
    height: 92px;
    line-height: 92px;
}
.supCover .oneImgBox .ImgBox{
    height: 164px;
    line-height: 164px;
}
.addCover .oneImgBox .myImgBox,.addCover .sinImgBox .myImgBox{
    display: inline-block;
    width: 164px;
    height: 92px;
    overflow: hidden;
}
.threeImgBox .sinImgBox .ImgBox{
    height: 92px;
    line-height: 92px;
}
.supCover .oneImgBox .myImgBox{
    display: inline-block;
    width: 164px;
    height: 164px;
    overflow: hidden;
}
/*弹出框*/
.dialogCover,.dialogCoverSup{
    width: 90%;
    height: 90%;
    position: fixed;
    top: 5%;
    left: 5%;
    border-radius: 4px;
    box-shadow: 0 0 5px #333;
    z-index: 1031;
    background: white;
    display: none;
    padding: 10px;
}

.dialogCover .selImgBtn,.dialogCoverSup .selImgBtn{
    margin-bottom: 10px;
}
.dialogCover .sureImgBtn,.dialogCoverSup .sureImgBtn{
    margin-bottom: 10px;
}
.dialogCover .closeImgBtn,.dialogCoverSup .closeImgBtn{
    margin-bottom: 10px;
}
.dialogCover .footer,.dialogCoverSup .footer {
    margin-top: 10px;
}
/*裁剪图片*/
.dialogCover .head {
    /*height: 600px;*/
    background-color: white;
}
.dialogCoverSup .head {
    /*height: 600px;*/
    background-color: white;
}
.dialogCover #target{
    min-width: 750px;
    max-width: 96%;
    /*max-height: 600px;*/
}
.dialogCoverSup #tarsup{
    /*min-width: 513px;*/
    /*max-width: 600px;*/
    min-height: 513px;
    max-height: 600px;
}

.dialogCover #preview-pane {
    display: none;
    position: fixed;
    top: 20px;
    right: 40px;
    width: 75px;
    height: 42px;
    overflow: hidden;
    border: 1px solid purple;
}
.dialogCover #preview-pane .preview-container {
    width: 100%;
    height: 100%;

}
.dialogCover canvas {
    display: none;
    position: fixed;
    top: 100px;
    right: 40px;
    border: 1px solid red;
    width: 750px;
    height: 422px;
}
.dialogCoverSup canvas {
    display: none;
    position: fixed;
    top: 100px;
    right: 40px;
    border: 1px solid red;
    width: 513px;
    height: 513px;
}

/*导语*/
.introduction{
    width: 90%;
}

/*目标人群*/
.selectBox,.classBox,.addBtnBox{
    margin-bottom: 10px;
}
.addBtnBox,.saveBtnBox{
    margin-top: 10px;
}
.isLocationLabe{
    margin-left: 20px;
}
.classBox .classSingle{
    overflow: hidden;
}
.classBox .classSingle .classParent{
    display: inline-block;
    width: 8%;
    float: left;
}
.classBox .classSingle .classChild{
    display: inline-block;
    width: 92%;
    float: left;
}
.classBox .classSingle .classChild label:nth-child(1){
    margin-left: 10px;
}
/*添加段落导语*/
.IntSingle{
    width: 100%;
    height: 150px;
    border: 1px solid #0d6aad;
    border-radius: 4px;
    margin-bottom: 10px;
}
.IntSingle .IntHead{
    line-height: 30px;
    border-bottom: 1px solid #C9C5C5;
    text-indent: 10px;
    overflow: hidden;
    cursor: move;
}
.IntSingle .IntHead .delBtn{
    display: inline-block;
    width: 28px;
    height: 28px;
    font-weight: bold;
    cursor: pointer;
    float: right;
    margin-right: 10px;
}
.IntSingle .textareaBox{
    padding: 10px;
    padding-top: 0;
    position: relative;
}
.IntSingle .textareaBox .des_tip{
    font-size: 14px;
    position: absolute;
    right: 20px;
    bottom: 16px;
    color: #999;
}
.IntSingle .textareaBox .des_tip.warning{
    color: #ed4040;
}
/*card_tip*/
.IntSingle .card_tip{
    font-size: 14px;
    position: absolute;
    right: 20px;
    bottom: 10px;
    color: #999;
}
.IntSingle .card_tip.warning{
    color: #ed4040;
}
.IntSingle textarea{
    width: 100%;
    resize:none;
}
.IntSingle .imgSingle{
    margin-left: 10px;
    margin-right: 10px;
    width: 100px;
    height: 100px;
}
.IntSingle .changeImgBtn{

}
