/*手机预览*/
.phone-preview{
    background-image: url(//p2.pstatp.com/large/pgc-image/153380077374313aa2d5949.png);
    background-size: cover;
    height: 690px;
    width: 356px;
    padding-top: 89px;
}
.preview-article-content{
    width: 287px;
    height: 496px;
    background-color: white;
    overflow-y: scroll;
    margin-left: 35px;
    padding: 0 15px;
}
.preview-article-content .tt-title{
    margin: 18px 0;
    font-size: 20px;
    color: #222222;
    font-weight: 400;
    line-height: 1.4;
}
.tt-leads,.article_img,.article_des,.article_card{
    margin-bottom: 10px;
}
.article_img .tt-img{
    width: 100%;
}
.article_card{
    border-bottom: 1px solid #d8d8d8;
    border-top: 1px solid #d8d8d8;
}
.article_card .tt-card{
    overflow: hidden;
    padding: 10px 0;
}
.article_card .tt-card .tt-card-left{
    float: left;
    width: 32.45%;
    margin-right: 12px;
}
.article_card .tt-card .tt-card-left .tt-img{
    width: 100%;
}
.article_card .tt-card .tt-card-right{
    overflow: hidden;
}
.tt-link{
    font-size: 12px;
    line-height: 14px!important;
    max-height: 42px;
    color: #222!important;
    word-wrap: break-word;
    font-weight: 400!important;
    text-overflow: ellipsis;
    overflow: hidden;
}
.tt-price{
    color: #3C8DBC;
    font-weight: 700;
    font-size: 12px;
    margin-bottom: 0;
}

/*陈梦婷---产品卡样式*/

.div_left{
    float: left;
    width: 15%;
    margin: 0 10px;
    padding-bottom: 10px;
    position: relative;
}
.div_center{
    float: left;
    width: 30%;
}
.div_right{
    float: right;
    width: 51%;
    padding-right: 10px;
    box-sizing: border-box;
    position: relative;
}
.goods_img{
    width: auto;
    height: 92px;
}
.change_img{
    position: absolute;
    display: inline-block;
    height: 20px;
    bottom: 10px;
    left: 0;
    line-height: 20px;
    text-align: center;
    background: rgba(0,0,0,0.8);
    color: white;
    text-align: center;
    cursor: pointer;
    font-size: 10px;
}
.IntSingle textarea.goods_des{
    width: 100%;
    height: 90px;
}

